{
  "compilerOptions": {
    "composite": true,
    "target": "ES2018",
    "lib": ["ES2018"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "incremental": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "../../typings",
  }
}